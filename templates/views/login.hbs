	<div class="main">
		<input type="checkbox" id="chk" aria-hidden="true">

			<div class="signup">
				<form action="/Login" method="POST" onsubmit="local(event)">
					<label for="chk" aria-hidden="true">Login</label>
					<input id="emai" type="email" name="email" placeholder="Email" required="">
					<input type="password" name="pswd" placeholder="Password" required="">
					<button>Login</button>

				</form>
			</div>
	</div>
</body>
